<h2> Edit Note for <%= @review.student.name %></h2>

<form method="post" action="/students/:<%=@review.student.id%>">
  <input id="hidden" type="hidden" name="_method" value="PATCH">
  <input id="hidden" type="hidden" name="review_id" value="<% @review.id %>">
  <input type="text" name="content" value="<%= @review.content %>" >
  <input type="submit" value="submit">
</form>




<a href="/account">Go Home</a><br>

